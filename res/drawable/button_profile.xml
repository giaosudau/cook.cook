<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#640101" />
            <stroke
                android:width="0.3dp"
                android:color="#696969" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#C2C2C2"
                android:endColor="#464545"
                android:angle="270" />
            <stroke
                android:width="0.3dp"
                android:color="#464545" />
            <corners
                android:radius="0dp" />

        </shape>
    </item>
</selector>